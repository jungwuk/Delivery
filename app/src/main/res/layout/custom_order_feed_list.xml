<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#FFF"
    android:orientation="vertical"
    >

    <LinearLayout
        android:id="@+id/feed_list_wrap"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="@drawable/box_gray"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="10dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#424242"
            android:orientation="vertical"
            android:padding="13dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/feed_list_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="주문사료 :"
                    android:textColor="#FFF"
                    android:textSize="16dp" />

                <TextView
                    android:id="@+id/feed_list_company_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#03A9F4"
                    android:layout_marginLeft="5dp"
                    android:text="[회사이름]"
                    android:textSize="16dp" />

                <TextView
                    android:id="@+id/feed_list_feed_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#FFF"
                    android:text="제품이름"
                    android:layout_marginLeft="5dp"
                    android:maxLines="1"
                    android:textSize="16dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/feed_list_type"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#0bd438"
                    android:background="@drawable/feed_green_box"
                    android:text="벌크"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:textSize="12dp" />

                <TextView
                    android:id="@+id/feed_list_section"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#0bd438"
                    android:background="@drawable/feed_green_box"
                    android:paddingRight="10dp"
                    android:paddingLeft="10dp"
                    android:text="비육돈"
                    android:layout_marginLeft="5dp"
                    android:textSize="12dp" />

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="10dp">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp">

                <TextView
                    android:id="@+id/tv_order_real"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="주문량"
                    android:textColor="#000"
                    android:textSize="15dp" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/feed_list_size"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:text=""
                        android:textColor="#1976D2"
                        android:textSize="15dp" />

                    <LinearLayout
                        android:id="@+id/feed_list_real_size_view_wrap"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/feed_list_real_size"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="right"
                            android:text=""
                            android:textColor="#1976D2"
                            android:textSize="15dp" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/feed_list_real_size_set_wrap"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/feed_list_real_size_set_input"
                            android:layout_width="70dp"
                            android:layout_height="wrap_content"
                            android:inputType="numberDecimal" />

                        <Button
                            android:id="@+id/feed_list_real_size_set_input_button"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="확인"
                            android:textColor="#FFF"
                            android:textSize="18dp" />

                    </LinearLayout>

                </LinearLayout>
            </FrameLayout>


            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="사료대금"
                    android:textColor="#000"
                    android:textSize="15dp" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/feed_list_price"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:textColor="#000"
                        android:textSize="15dp" />

                    <!--  <TextView
                          android:id="@+id/feed_list_price_exp"
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:text="0"
                          android:textColor="#AAA"
                          android:textSize="14dp" />-->

                </LinearLayout>

            </FrameLayout>
        </LinearLayout>
    </LinearLayout>


    <LinearLayout

        android:id="@+id/feed_option_wrap"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="@drawable/box_gray"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/box_gray_feed_detail"
            android:orientation="vertical"
            android:padding="13dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/feed_option_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#101010"
                    android:textSize="16dp" />

                <TextView
                    android:id="@+id/feed_option_company_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:textColor="#03A9F4"
                    android:textSize="16dp" />

                <TextView
                    android:id="@+id/feed_option_feed_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:textColor="#101010"
                    android:textSize="16dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="10dp"
                >

                <TextView
                    android:id="@+id/feed_option_type"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#0bd438"
                    android:background="@drawable/feed_green_box"
                    android:text="벌크"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:textSize="12dp" />

                <TextView
                    android:id="@+id/feed_option_section"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#0bd438"
                    android:background="@drawable/feed_green_box"
                    android:paddingRight="10dp"
                    android:paddingLeft="10dp"
                    android:text="비육돈"
                    android:layout_marginLeft="5dp"
                    android:textSize="12dp" />

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="10dp">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="주문량/실공급량"
                    android:textColor="#000"
                    android:textSize="15dp" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/feed_option_size"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:text=""
                        android:textColor="#1976D2"
                        android:textSize="15dp" />

                    <LinearLayout
                        android:id="@+id/feed_option_real_size_view_wrap"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/feed_option_real_size"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="right"
                            android:text=""
                            android:textColor="#1976D2"
                            android:textSize="15dp" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/feed_option_real_size_set_wrap"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/feed_option_real_size_set_input"
                            android:layout_width="70dp"
                            android:layout_height="wrap_content"
                            android:inputType="numberDecimal" />

                        <Button
                            android:id="@+id/feed_option_real_size_set_input_button"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="확인"
                            android:textColor="#FFF"
                            android:textSize="18dp" />

                    </LinearLayout>

                </LinearLayout>
            </FrameLayout>


            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="사료대금"
                    android:textColor="#000"
                    android:textSize="15dp" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/feed_option_price"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:textColor="#000"
                        android:textSize="15dp" />

                    <!-- <TextView
                         android:id="@+id/feed_option_price_exp"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"
                         android:text="0"
                         android:textColor="#AAA"
                         android:textSize="14dp" />-->

                </LinearLayout>

            </FrameLayout>
        </LinearLayout>
    </LinearLayout>


    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/box_gray_feed_detail"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="5dp"
        android:padding="7dp">


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left"
            android:orientation="horizontal"
            android:paddingLeft="5dp"
            android:paddingRight="10dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="옵션 : "
                android:textColor="#000"
                android:textSize="18dp" />

            <TextView
                android:id="@+id/feed_nutrient"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0"
                android:textColor="#000"
                android:textSize="18dp" />


        </LinearLayout>

    </FrameLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:orientation="vertical"></LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="10dp"
        android:background="#CCC"
        android:visibility="gone">

    </LinearLayout>

</LinearLayout>